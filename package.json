{
  "name": "styler-syntax-highlighter",
  "displayName": "Styler Syntax Highlighter",
  "description": "VS Code extension for custom Styler syntax highlighting",
  "version": "1.0.0",
  "publisher": "TheSeekerFox",
  "engines": {
    "vscode": ">=1.50.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheSeekerFox/vscode-styler-syntax.git"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "styler",
        "aliases": ["Styler", "styler"],
        "filenames": [
          ".styler",
          ".stylerc"
        ],
        "extensions": [
          ".styler"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "styler",
        "scopeName": "source.styler",
        "path": "./syntaxes/styler.tmLanguage.json"
      }
    ]
  },
  "activationEvents": ["onLanguage:styler"],
  "main": "./extension.js"
}
